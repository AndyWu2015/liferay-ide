<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.wst.server.ui.serverImages">
      <image
            icon="icons/e16/server.png"
            id="com.liferay.ide.eclipse.server.tomcat.ui.image"
            typeIds="com.liferay.ide.eclipse.server.tomcat.runtime.60">
      </image>
      <image
            icon="icons/e16/server.png"
            id="com.liferay.ide.eclipse.server.tomcat.runtime.ui.image"
            typeIds="com.liferay.ide.eclipse.server.tomcat.60">
      </image>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="com.liferay.ide.eclipse.server.tomcat.60.launchConfigurationType"
            icon="icons/e16/server.png"
            id="com.liferay.ide.eclipse.server.tomcat.ui.launchConfigurationTypeImage">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="com.liferay.ide.eclipse.server.tomcat.ui.PortalTomcatLaunchConfigTabGroup"
            id="com.liferay.ide.eclipse.server.tomcat60.ui.launchConfigurationTabGroup"
            type="com.liferay.ide.eclipse.server.tomcat.60.launchConfigurationType">
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.wst.server.ui.wizardFragments">
      <fragment
            class="com.liferay.ide.eclipse.server.tomcat.ui.wizard.PortalTomcatRuntimeFragment"
            id="com.liferay.ide.eclipse.server.tomcat.runtime.60"
            typeIds="com.liferay.ide.eclipse.server.tomcat.runtime.60">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="org.eclipse.jst.server.tomcat.ui.internal.ConfigurationPropertyTester"
            id="com.liferay.ide.eclipse.server.tomcat.ui.propertyTester"
            namespace="com.liferay.ide.eclipse.server.tomcat.ui"
            properties="hasConfiguration"
            type="java.lang.Object">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.wst.server.core.publishers">
      <publisher
            class="org.eclipse.jst.server.tomcat.ui.internal.ContextPublisherDelegate"
            description="Prompt to update context paths when they are out of sync."
            id="com.liferay.ide.eclipse.server.tomcat.ui.context"
            name="Update context path"
            order="0"
            typeIds="com.liferay.ide.eclipse.server.tomcat.*">
      </publisher>
   </extension>
   <extension
         point="org.eclipse.wst.server.ui.editorPageSections">
      <section
            class="com.liferay.ide.eclipse.server.tomcat.ui.editor.PortalServerLocationEditorSection"
            id="com.liferay.ide.eclipse.server.tomcat.editor.instance"
            insertionId="org.eclipse.wst.server.editor.overview.left"
            order="0"
            typeIds="com.liferay.ide.eclipse.server.tomcat.*">
      </section>
      <!--
      <section
            class="com.liferay.ide.eclipse.server.tomcat.ui.editor.PortalServerGeneralEditorSection"
            id="com.liferay.ide.eclipse.server.tomcat.editor.general"
            insertionId="org.eclipse.wst.server.editor.overview.left"
            order="5"
            typeIds="com.liferay.ide.eclipse.server.tomcat.*">
      </section>
      -->
      <section
            class="org.eclipse.jst.server.tomcat.ui.internal.editor.ConfigurationMimeEditorSection"
            id="com.liferay.ide.eclipse.server.tomcat.configuration.editor.mime"
            insertionId="org.eclipse.wst.server.editor.overview.right"
            order="20"
            typeIds="com.liferay.ide.eclipse.server.tomcat.*">
         <enablement>
            <test
                  property="com.liferay.ide.eclipse.server.tomcat.ui.hasConfiguration"
                  value="true">
            </test>
         </enablement>
      </section>
      <section
            class="org.eclipse.jst.server.tomcat.ui.internal.editor.ConfigurationPortEditorSection"
            id="com.liferay.ide.eclipse.server.tomcat.configuration.editor.port"
            insertionId="org.eclipse.wst.server.editor.overview.right"
            order="10"
            typeIds="com.liferay.ide.eclipse.server.tomcat.*">
         <enablement>
            <test
                  property="com.liferay.ide.eclipse.server.tomcat.ui.hasConfiguration"
                  value="true">
            </test>
         </enablement>
      </section>
   </extension>
   <extension
         point="org.eclipse.wst.common.project.facet.core.runtimes">
      <adapter>
         <runtime-component
               id="com.liferay.ide.eclipse.server.tomcat.runtime.component">
         </runtime-component>
         <factory
               class="com.liferay.ide.eclipse.server.tomcat.ui.PortalTomcatRuntimeComponentLabelProvider">
         </factory>
         <type
               class="org.eclipse.wst.common.project.facet.ui.IRuntimeComponentLabelProvider">
         </type>
      </adapter>
      <adapter>
         <runtime-component
               id="com.liferay.ide.eclipse.server.tomcat.runtime.component">
         </runtime-component>
         <factory
               class="com.liferay.ide.eclipse.server.tomcat.ui.PortalTomcatRuntimeDecorationsProvider">
         </factory>
         <type
               class="org.eclipse.wst.common.project.facet.ui.IDecorationsProvider">
         </type>
      </adapter>
   </extension>
   <extension
         point="org.eclipse.wst.common.project.facet.ui.images">
      <image
            path="icons/e16/server.png"
            runtime-component-type="com.liferay.ide.eclipse.server.tomcat.runtime.component">
      </image>
   </extension>

</plugin>
